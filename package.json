{
  "name": "cafe-clique-v1.0",
  "version": "1.0.0",
  "description": "A social media app for teens to find local friends with the same vibe",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run frontend:dev\" \"npm run backend:dev\"",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:dev": "cd backend && nodemon server.js",
    "build": "npm run frontend:build && npm run backend:build",
    "frontend:build": "cd frontend && npm run build",
    "start": "cd backend && node server.js"
  },
  "keywords": [
    "social-media",
    "teens",
    "friend-finder",
    "local",
    "matching"
  ],
  "author": "Tejas Kamble",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
